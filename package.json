{
  "name": "vscode-svn-ai",
  "displayName": "VSCode SVN - AI智能版本控制",
  "description": "🚀 全平台SVN智能插件：基于原生命令行工具，支持Windows/macOS/Linux，内置AI提交日志生成，可视化差异对比，100%开源透明。无需TortoiseSVN，轻量级高性能！",
  "version": "4.8.6",
  "publisher": "pengfeiSummer",
  "icon": "resources/icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "SCM Providers",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onCommand:vscode-svn.uploadFile",
    "onCommand:vscode-svn.uploadFolder",
    "onCommand:vscode-svn.commitFile",
    "onCommand:vscode-svn.revertFile",
    "onCommand:vscode-svn.revertFolder",
    "onCommand:vscode-svn.setSvnRoot",
    "onCommand:vscode-svn.clearSvnRoot",
    "onCommand:vscode-svn.updateFile",
    "onCommand:vscode-svn.updateDirectory",
    "onCommand:vscode-svn.updateWorkspace",
    "onCommand:vscode-svn.viewLog",
    "onCommand:vscode-svn.showLocalRevision",
    "onCommand:vscode-svn.configureFilter",
    "onCommand:vscode-svn.showFilterInfo",
    "onCommand:vscode-svn.checkout",
    "workspaceContains:.svn"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-svn.uploadFile",
        "title": "SVN: 上传文件"
      },
      {
        "command": "vscode-svn.revertFile",
        "title": "SVN: 恢复文件"
      },
      {
        "command": "vscode-svn.revertFolder",
        "title": "SVN: 恢复文件夹"
      },
      {
        "command": "vscode-svn.uploadFolder",
        "title": "SVN: 上传文件夹"
      },
      {
        "command": "vscode-svn.commitFile",
        "title": "SVN: 提交文件(显示差异)"
      },
      {
        "command": "vscode-svn.setSvnRoot",
        "title": "SVN: 设置SVN工作副本路径"
      },
      {
        "command": "vscode-svn.clearSvnRoot",
        "title": "SVN: 清除SVN工作副本路径"
      },
      {
        "command": "vscode-svn.updateFile",
        "title": "SVN: 更新文件"
      },
      {
        "command": "vscode-svn.updateDirectory",
        "title": "SVN: 更新目录"
      },
      {
        "command": "vscode-svn.updateWorkspace",
        "title": "SVN: 更新工作区"
      },
      {
        "command": "vscode-svn.viewLog",
        "title": "SVN: 查看日志记录"
      },
      {
        "command": "vscode-svn.showLocalRevision",
        "title": "SVN: 显示本地修订版本号"
      },
      {
        "command": "vscode-svn.configureFilter",
        "title": "SVN: 配置过滤规则"
      },
      {
        "command": "vscode-svn.showFilterInfo",
        "title": "SVN: 显示过滤信息"
      },
      {
        "command": "vscode-svn.showAICacheStats",
        "title": "SVN: 显示AI缓存统计"
      },
      {
        "command": "vscode-svn.clearAICache",
        "title": "SVN: 清空AI缓存"
      },
      {
        "command": "vscode-svn.cleanExpiredAICache",
        "title": "SVN: 清理过期AI缓存"
      },
      {
        "command": "vscode-svn.configureAI",
        "title": "SVN: 配置AI服务"
      },
      {
        "command": "vscode-svn.checkout",
        "title": "SVN: 检出|Checkout 地址"
      },
      {
        "command": "vscode-svn.manageCredentials",
        "title": "SVN: 管理认证信息"
      },
      {
        "command": "vscode-svn.clearCredentials",
        "title": "SVN: 清除认证信息"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "vscode-svn.showLocalRevision",
          "when": "resourceScheme == file",
          "group": "2_svn@1"
        },
        {
          "command": "vscode-svn.uploadFile",
          "when": "resourceScheme == file && !explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.revertFile",
          "when": "resourceScheme == file && !explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.revertFolder",
          "when": "explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.uploadFolder",
          "when": "explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.commitFile",
          "when": "resourceScheme == file && !explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.updateFile",
          "when": "resourceScheme == file && !explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.updateDirectory",
          "when": "explorerResourceIsFolder",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.viewLog",
          "when": "resourceScheme == file",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.setSvnRoot",
          "when": "explorerResourceIsFolder",
          "group": "2_svn@9"
        },
        {
          "command": "vscode-svn.checkout",
          "when": "explorerResourceIsFolder || !resourceScheme",
          "group": "2_svn@10"
        }
      ],
      "editor/context": [
        {
          "command": "vscode-svn.showLocalRevision",
          "when": "resourceScheme == file",
          "group": "2_svn@1"
        },
        {
          "command": "vscode-svn.commitFile",
          "when": "resourceScheme == file",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.revertFile",
          "when": "resourceScheme == file",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.updateFile",
          "when": "resourceScheme == file",
          "group": "2_svn"
        },
        {
          "command": "vscode-svn.viewLog",
          "when": "resourceScheme == file",
          "group": "2_svn"
        }
      ],
      "commandPalette": [
        {
          "command": "vscode-svn.setSvnRoot",
          "group": "svn"
        },
        {
          "command": "vscode-svn.clearSvnRoot",
          "group": "svn"
        },
        {
          "command": "vscode-svn.updateWorkspace",
          "group": "svn"
        },
        {
          "command": "vscode-svn.showLocalRevision",
          "group": "svn"
        },
        {
          "command": "vscode-svn.configureFilter",
          "group": "svn"
        },
        {
          "command": "vscode-svn.showFilterInfo",
          "group": "svn"
        },
        {
          "command": "vscode-svn.configureAI",
          "group": "svn"
        },
        {
          "command": "vscode-svn.checkout",
          "group": "svn"
        },
        {
          "command": "vscode-svn.manageCredentials",
          "group": "svn"
        },
        {
          "command": "vscode-svn.clearCredentials",
          "group": "svn"
        }
      ],
      "view/title": [
        {
          "command": "vscode-svn.updateWorkspace",
          "when": "view == explorer",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "VSCode SVN",
      "properties": {
        "vscode-svn.customSvnRoot": {
          "type": "string",
          "default": null,
          "description": "自定义SVN工作副本根目录路径"
        },
        "vscode-svn.aiApiUrl": {
          "type": "string",
          "default": "",
          "description": "AI服务的API访问地址"
        },
        "vscode-svn.aiModelId": {
          "type": "string",
          "default": "",
          "description": "AI模型ID或名称"
        },
        "vscode-svn.aiApiKey": {
          "type": "string",
          "default": "",
          "description": "AI服务的API密钥"
        },
        "vscode-svn.showUncommittedChanges": {
          "type": "boolean",
          "default": true,
          "description": "在日志列表中显示未提交到本地的修改"
        },
        "vscode-svn.excludeFiles": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "*.log",
            "*.tmp",
            "node_modules",
            ".DS_Store",
            "Thumbs.db"
          ],
          "description": "排除的文件和文件夹模式列表（支持glob模式）"
        },
        "vscode-svn.excludeFolders": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "node_modules",
            ".git",
            ".vscode",
            "dist",
            "build",
            "out",
            "target"
          ],
          "description": "排除的文件夹列表"
        },
        "vscode-svn.defaultFileEncoding": {
          "type": "string",
          "enum": [
            "auto",
            "utf8",
            "gbk",
            "gb2312",
            "big5",
            "latin1"
          ],
          "default": "auto",
          "description": "默认文件编码格式。auto表示自动检测，其他选项为强制使用指定编码"
        },
        "vscode-svn.forceUtf8Output": {
          "type": "boolean",
          "default": true,
          "description": "强制SVN命令输出使用UTF-8编码。建议保持开启以避免中文乱码"
        },
        "vscode-svn.enableEncodingDetection": {
          "type": "boolean",
          "default": true,
          "description": "启用文件编码自动检测。关闭后将使用默认编码"
        },
        "vscode-svn.encodingFallbacks": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "utf8",
            "gbk",
            "gb2312",
            "big5"
          ],
          "description": "编码检测失败时的备用编码列表"
        },
        "vscode-svn.showEncodingInfo": {
          "type": "boolean",
          "default": false,
          "description": "在差异对比界面显示文件编码信息"
        },
        "vscode-svn.authenticationConfig.autoSaveCredentials": {
          "type": "boolean",
          "default": true,
          "description": "自动保存SVN认证信息"
        },
        "vscode-svn.authenticationConfig.defaultAuthPrompt": {
          "type": "boolean",
          "default": true,
          "description": "认证失败时自动提示输入用户名密码"
        },
        "vscode-svn.authenticationConfig.credentialTimeout": {
          "type": "number",
          "default": 86400,
          "description": "认证信息有效期（秒，0表示永不过期）"
        },
        "vscode-svn.authenticationConfig.serverPatterns": {
          "type": "object",
          "default": {},
          "description": "服务器URL匹配模式配置",
          "patternProperties": {
            ".*": {
              "type": "string",
              "description": "匹配模式对应的服务器根URL"
            }
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run copy-templates",
    "copy-templates": "mkdir -p out/templates && cp -r src/templates/* out/templates/ && mkdir -p templates && cp -r src/templates/* templates/",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/summer198971/vscode-svn.git"
  },
  "bugs": {
    "url": "https://github.com/summer198971/vscode-svn/issues"
  },
  "homepage": "https://github.com/summer198971/vscode-svn",
  "keywords": [
    "svn",
    "subversion",
    "全平台",
    "cross-platform",
    "AI智能",
    "artificial intelligence",
    "版本控制",
    "version control",
    "差异对比",
    "diff comparison",
    "开源",
    "open source",
    "macOS",
    "Linux",
    "Windows",
    "智能提交",
    "smart commit",
    "代码分析",
    "code analysis",
    "命令行",
    "command line",
    "轻量级",
    "lightweight",
    "原生",
    "native"
  ],
  "author": "summer198971",
  "license": "MIT",
  "dependencies": {
    "@types/xmldom": "^0.1.34",
    "@xmldom/xmldom": "^0.9.8",
    "minimatch": "^9.0.3",
    "openai": "^6.7.0",
    "vsce": "^2.15.0"
  },
  "devDependencies": {
    "@types/minimatch": "^5.1.2",
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.5.5"
  }
}
